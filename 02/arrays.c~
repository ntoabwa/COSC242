    double judge_2[ARRAY_SIZE];
    double judge_3[ARRAY_SIZE];
    double a, b, c;
    int lines = 0;
    int i = 0;
    double total_1 = 0;
    double total_2 = 0;
    double total_3 = 0;
    
    while (4 == scanf("%d%lg%lg%lg", &n, &a, &b, &c)) {
            for ( int i = 0; i < ARRAY_SIZE; i++) {
                judge_1[i] = a;
                judge_2[i] = b;
                judge_3[i] = c;
            }
            lines++;
    }

