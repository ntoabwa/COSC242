#include <stdio.h>
#include <stdlib.h>

int factorial(int num) {

    if (n == 0) {
        return 1;
    } else {
        return n * factorial(n -1);
    }
    
              
}


int main() {
    int num;

    while (1 == scanf("%d", &num)) {
        printf("%d\n", factorial(num));
    }

    return EXIT_SUCCESS;
}
